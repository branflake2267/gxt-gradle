<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
    "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
  <inherits name="com.sencha.gxt.themebuilder.base.ThemeBase"/>

  <extend-property name="gxt.theme" values="custom"/>
  <set-property name="gxt.theme" value="custom"/>

  <!-- Constants decl - generated instance used to build theme -->
  <replace-with class="com.gawkat.customtheme.client.ThemeDetailsImpl">
    <when-type-is class="com.sencha.gxt.themebuilder.base.client.config.ThemeDetails"/>
    <when-property-is name="gxt.theme" value="custom"/>
  </replace-with>

  <!-- Default is Sliced+Css3.  use slicedDisabled flag to use only Css3 version -->
  <inherits name="com.gawkat.customtheme.Sliced" />
</module>
